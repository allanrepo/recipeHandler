    {
    dig_ac_ck	    f.group_syntax "add dig_ac_ck"
    dig_ck	    f.group_syntax "add dig_ck"
    dig_ctmu_cal    f.group_syntax "add dig_ctmu_cal"
    dig_ctmu_ck     -group dig_ctmu_ck_hf f.syntax "add dig_ctmu_ck"
    dig_ctmu_ck     f.group_syntax "add dig_ctmu_ck"
    dig_farm_ck     f.group_syntax "add dig_farm_ck"
    dig_levels_cal  f.group_syntax "add dig_levels_cal"
    dig_levels_ck   -group dig_levels_ck_hf f.syntax "add dig_levels_ck"
    dig_levels_ck   f.group_syntax "add dig_levels_ck"
    dig_memory_ck   f.group_syntax "add dig_memory_ck"
    dig_pati_ck     f.group_syntax "add dig_pati_ck"
    dig_pec_ck      f.group_syntax "add dig_pec_ck"
    dig_peic_ck	    f.group_syntax "add dig_peic_ck"
    dig_pogo_ck     -group dig_pogo_ck_hf f.syntax "add dig_pogo_ck"
    dig_pogo_ck     f.group_syntax "add dig_pogo_ck"
    dig_ppmu_cal    f.group_syntax "add dig_ppmu_cal"
    dig_ppmu_ck     -group dig_ppmu_ck_hf f.syntax "add dig_ppmu_ck"
    dig_ppmu_ck     f.group_syntax "add dig_ppmu_ck"
    dig_quick_ck    f.group_syntax "add dig_quick_ck"
    dig_register_ck f.group_syntax "add dig_register_ck"
