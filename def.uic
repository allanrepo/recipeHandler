Menu diagOptionsMenu -title "Diagnostics Selection Options"
    {
    "Columns"       f.menu columnMenu
    "Select"        f.menu selectMenu
    "View"          f.menu viewMenu
    "Menus"         f.menu showMenu
    }

Menu columnMenu -title "Column Menu"
    {
    1     f.syntax "column 1"
    2     f.syntax "column 2"
    3     f.syntax "column 3"
    4     f.syntax "column 4"
    5     f.syntax "column 5"
    6     f.syntax "column 6"
    Auto  f.syntax "column auto"
    }

Menu viewMenu -title "View Menu"
    {
    All              f.syntax "view all"
    Date             f.syntax "view date"
    Time             f.syntax "view time"
    Result           f.syntax "view result"
    "Date & Time"    f.syntax "view date_time"
    "Date & Result"  f.syntax "view date_result"
    "Time & Result"  f.syntax "view time_result"
    "Name Only"      f.syntax "view name"
    }

Menu selectMenu -title "Select Menu"
    {
    All            f.syntax "select all"
    "Fail Only"    f.syntax "select fail_only"
!    "Marginal Only" f.syntax "select marginal_only"
    "Halt Only"    f.syntax "select halt_only"
    "Out of date"  f.syntax "select out_date"
    "Pass Only"    f.syntax "select pass_only"
    "Execute Only" f.syntax "select execute_only"
    }

Menu showMenu -title "Show Menu"
    {
    "Hide inactive items"    f.syntax "show_diags active"
    "Show inactive items"    f.syntax "show_diags all"
    }
