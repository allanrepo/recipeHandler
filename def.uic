    "To File"               f.dialog summaryFile
    "To printer"            f.syntax "diag_summary printer"
    "Clear Summary"         f.syntax "diag_summary clear"
    }

Dialog summaryFile -title "Print summary to file"
    {
    "File name:"       -format "diag_summary file %s" f.pathname
    }


